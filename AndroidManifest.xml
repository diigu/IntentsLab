<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.testapp1"
          android:versionCode="1"
          android:versionName="1.0">

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="15"/>

    <uses-permission android:name="android.permission.GET_TASKS"/>
    <uses-permission android:name="android.permission.BROADCAST_STICKY"/>

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".editor.IntentEditorActivity"
            android:windowSoftInputMode="stateHidden">
        </activity>
        <activity android:name=".editor.EntryEditorActivity">
        </activity>
        <activity
            android:name=".PickRecentlyRunningActivity"
            android:label="@string/pick_recent_task">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.testapp1.StartActivity"/>
        </activity>
        <activity
            android:name=".browser.BrowseComponentsActivity"
            android:label="@string/browse_components">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.testapp1.StartActivity"/>
        </activity>
        <activity
            android:name=".CatchBroadcastDialog$WrapperActivity"
            android:theme="@style/NoDisplayForDialogTheme"/>

        <service android:name=".CatchBroadcastService"/>

        <activity android:name=".PrefsActivity"/>
        <activity
            android:name=".browser.ComponentInfoActivity"
            android:label="@string/component_info"/>
        <activity android:name=".XMLViewerActivity"/>
        <activity android:name=".providerlab.ProviderInfoActivity"/>
        <activity android:name=".providerlab.QueryResultActivity"/>
        <activity android:name=".providerlab.ProviderLabActivity"/>

        <receiver
            android:name=".samplecomponents.AbortBroadcastReceiver"
            android:label="Sample abortBroadcast()">
            <intent-filter>
                <action android:name="sample.abortbroadcast" />
            </intent-filter>
        </receiver>
        <receiver
            android:name=".samplecomponents.SetResultReceiver"
            android:label="Sample setResult()">
            <intent-filter>
                <action android:name="sample.setresult" />
            </intent-filter>
        </receiver>
    </application>

</manifest>